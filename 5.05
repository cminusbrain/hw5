#include <iostream>
#include <ctime>
#include <cmath>

int main()
{
    int a, b, n;
    std::cin >> n >> a >> b;
    switch (n)
    {
        case 1: std::cout << a + b; break;
        case 2: std::cout << a - b; break;
        case 3: std::cout << a * b; break;
        case 4: std::cout << a / b; break;
    }
    return 0;
}
