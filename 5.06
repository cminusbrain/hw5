#include <iostream>
#include <ctime>
#include <cmath>

int main()
{
    int l, n;
    std::cin >> n >> l;
    switch (n)
    {
        case 1: std::cout << l / 10; break;
        case 2: std::cout << l * 1000; break;
        case 3: std::cout << l; break;
        case 4: std::cout << l / 1000; break;
        case 5: std::cout << l / 100; break;
    }
    return 0;
}
